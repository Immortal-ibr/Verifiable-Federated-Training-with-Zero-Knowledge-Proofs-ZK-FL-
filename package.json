{
  "name": "zk-balance",
  "version": "1.0.0",
  "description": "Zero-Knowledge Proof of Dataset Balance - Semester Project",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "setup:circuits": "bash scripts/setup-circuits.sh",
    "compile:circuits": "circom zk/circuits/balance.circom --r1cs --wasm --sym -o build/circuits",
    "generate-data": "tsx scripts/generate-data.ts",
    "commit": "tsx scripts/commit.ts",
    "prove": "tsx scripts/prove.ts",
    "verify": "tsx scripts/verify.ts",
    "demo": "tsx scripts/demo.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf build dist data/*.json data/*.csv",
    "format": "prettier --write 'src/**/*.ts' 'scripts/**/*.ts'",
    "lint": "eslint 'src/**/*.ts' 'scripts/**/*.ts'"
  },
  "keywords": [
    "zero-knowledge",
    "zk-snark",
    "circom",
    "privacy",
    "cryptography",
    "merkle-tree"
  ],
  "authors": [
    "Tarek Ibrahim",
    "Zeyad ElShafey",
    "Ahmed ELbehiry"
  ],
  "license": "MIT",
  "dependencies": {
    "snarkjs": "^0.7.3",
    "ffjavascript": "^0.2.63",
    "circomlibjs": "^0.1.7",
    "commander": "^11.1.0",
    "csv-parse": "^5.5.3",
    "csv-stringify": "^6.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
